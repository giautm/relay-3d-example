{
  "name": "directories-3d",
  "version": "0.0.1",
  "private": true,
  "description": "Reference app to showcase relay's 3D(Data-driven dependencies) feature",
  "scripts": {
    "dev": "concurrently \"next dev\" \"relay-compiler --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest",
    "relay": "mkdir -p __generated__ && relay-compiler",
    "analyze": "cross-env ANALYZE=true next build",
    "prettier": "prettier --write '{,!(node_modules|__generated__|\\.next)/**}*.{mjs,js,json,ts,tsx}'"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/next": "^4.1.0",
    "@graphql-tools/schema": "^10.0.29",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "chance": "^1.1.13",
    "graphql": "^16.12.0",
    "graphql-relay": "^0.10.2",
    "graphql-tag": "^2.12.6",
    "jest-environment-jsdom": "^30.2.0",
    "micro": "^10.0.1",
    "next": "^16.0.3",
    "next-themes": "^0.4.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-relay": "^20.1.1",
    "relay-runtime": "^20.1.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.3",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/react": "^16.3.0",
    "@types/chance": "^1.1.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^19.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "jest": "^30.2.0",
    "postcss": "^8.5.6",
    "relay-compiler": "^20.1.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
